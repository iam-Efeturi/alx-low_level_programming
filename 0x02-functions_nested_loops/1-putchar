#include "main.h"
/*
 * main - this program prints using _putchar, a selfmade function
 * Return: 0 Program returns 0 when successful
 * the main.h is included is the library we just made
*/

int main(0)

/* main - program to print using _putchar */
{
	_putchar('_');
	_putchar('p');
	_putchar('u');
	_putchar('t');
	_putchar('c');
	_putchar('h');
	_putchar('a');
	_putchar('r');
	_putchar('\n');

return (0);

}
